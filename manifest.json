{
// Required
  "manifest_version": 2,
  "name": "GCal Hide",
  "version": "0.0.1",

  // Recommended
  "default_locale": "en",
  "description": "Lets you hide google calendar events",

  "content_scipts": [
  	{
  		"matches": ["https://www.google.com/calendar/*"],
  		"css": ["style.css"],
  		"js": ["dependencies/jquery.js", "calendar.js"]
  	}
  ]
}